func main()
{
  print(1 + 2);
  print(1 + 2.0);
  print(1.0 + 2);
  print(1.0 + 2.0);

  print(1 - 2);
  print(1 - 2.0);
  print(1.0 - 2);
  print(1.0 - 2.0);

  print(6 * 2);
  print(6 * 2.0);
  print(6.0 * 2);
  print(6.0 * 2.0);

  print(6 / 2);
  print(6 / 2.0);
  print(6.0 / 2);
  print(6.0 / 2.0);
}
